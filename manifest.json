{
  "manifest_version": 2,
  "name": "Smart Shopper",
  "version": "1.0.4",
  "description": "A universal shopping cart to allow cross-site shopping research",
  
  "background":{
    "page": "bg.html"
    },

  
  
    "browser_action": {
    "default_icon": "icon19.png",
    "default_title": "Smart Shopper"
    },

  
  "permissions" : ["http://*/*","tabs","contextMenus","https://*/*","chrome://favicon/*","unlimitedStorage","tabCapture"],
  "minimum_chrome_version": "23"

}